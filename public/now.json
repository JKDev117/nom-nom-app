{
    "version": 2,
    "name": "nom-nom-app",
    "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "^/images/(.*)",
            "dest": "/images/$1"
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }       
    ]
}


